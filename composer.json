{
    "name": "delisend/woocommerce-delisend",
    "description": "Delisend Woocommerce Rest APi client",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "delisend",
        "wordpress",
        "api"
    ],
    "homepage": "https://delisend.com/",
    "authors": [{
        "name": "Rosi Group s.r.o.",
        "email": "contact@rosigroup.com"
    }],
    "autoload": {
